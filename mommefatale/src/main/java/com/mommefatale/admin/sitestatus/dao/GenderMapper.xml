<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mommefatale.admin.sitestatus.dao.GenderMapper">

	<select id="manCount" resultType="java.lang.Integer">
		select count(*) from member where member_gender = '남'
	</select>
	<select id="womanCount" resultType="java.lang.Integer">
		select count(*) from member where member_gender = '여'
	</select>
</mapper>